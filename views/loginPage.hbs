<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/main.css">
    <title>Share with me</title>
</head>

<body>
    <section class="log">
        <div class="reg_page">
            <img src="/img/SWMXLlogo.jpg" alt="share with me logo" >
            <div class="form-data">
                <h1>Share <span class="title-log">With me</span></h1>
                <div class="user-name">
                    <input type="email" class="userName" id="userName" placeholder="Email">
                </div>
                <div class="password">
                    <input type="password" class= "password_input" id="password" placeholder="Password">
                </div>
                <p class="message-ident"></p>
                <div class="button-login">
                    <a class="butn-login">Log in</a>
                </div>
                <hr class="identifier">
                <div class="login_reg">
                    Don't have an account?
                    <a class="btn_reg" href="/registration">Registration</a>
                </div>
            </div>
        </div>
    </section>
</body>

</html>